void switchMe(int x)
{
    switch(x)
    {
        case 0: {
            print("zero");
            return;
        } break;
        case 1: {
            print("one");
            return;
        } break;
        case 2: {
            print("two");
            return;
        } break;
    };
    print("practically infinity");
}

